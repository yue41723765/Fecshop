<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tool="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="306px"
    android:id="@+id/rl_all"
    android:background="@drawable/vpi__tab_unselected_holo">

    <TextView
        android:id="@+id/tv_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:lines="1"
        android:ellipsize="end"
        android:textSize="30px"
        android:layout_marginTop="36px"
        android:layout_marginLeft="61px"
        android:gravity="left"
        android:layout_toLeftOf="@id/cb_default"
        android:textColor="#000"
        tool:text="Zhang xiaobai"/>
    <CheckBox
        android:id="@+id/cb_default"
        android:layout_width="30px"
        android:layout_height="30px"
        android:layout_alignParentRight="true"
        android:layout_marginRight="37px"
        android:layout_marginTop="37px"
        android:button="@null"
        android:clickable="false"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:background="@drawable/selector_all"
        />
    <TextView
        android:id="@+id/tv_city"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="47px"
        android:layout_marginLeft="62px"
        android:layout_below="@id/tv_name"
        android:textSize="24px"
        android:textColor="@color/text_118"
        tool:text="北京市朝阳区"/>

    <TextView
        android:id="@+id/tv_code"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/tv_city"
        android:layout_below="@id/tv_name"
        android:layout_marginTop="47px"
        android:layout_marginLeft="41px"
        android:textSize="24px"
        android:textColor="@color/text_117"
        tool:text="10001" />
    <TextView
        android:id="@+id/tv_address"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tv_city"
        android:layout_marginTop="26px"
        android:layout_marginLeft="62px"
        android:layout_marginRight="62px"
        android:textSize="24px"
        android:textColor="@color/text_117"
        android:lines="1"
        android:ellipsize="end"
        tool:text="北京市朝阳区双井街道百子园2010"
        />
    <TextView
        android:id="@+id/tv_phone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#000"
        android:layout_below="@id/tv_address"
        android:layout_marginTop="40px"
        android:layout_marginLeft="62px"
        android:textSize="24px"
        tool:text="18536500269"/>
    
    <TextView
        android:id="@+id/tv_delete"
        android:layout_width="90px"
        android:layout_height="50px"
        android:textColor="@color/text_122"
        android:textSize="17px"
        android:background="@drawable/shape_pw"
        android:text="@string/delete"
        android:gravity="center"
        android:layout_alignParentBottom="true"
        android:layout_toLeftOf="@id/tv_edit"
        android:layout_marginBottom="26px"
        android:layout_marginRight="14px"
        android:onClick="onClick"/>
    <TextView
        android:id="@+id/tv_edit"
        android:layout_width="90px"
        android:layout_height="50px"
        android:textColor="@color/text_122"
        android:textSize="17px"
        android:background="@drawable/shape_pw"
        android:text="@string/edit"
        android:gravity="center"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="26px"
        android:layout_marginRight="36px"
        android:onClick="onClick"/>

</RelativeLayout>